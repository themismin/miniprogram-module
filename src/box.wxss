/* 全局修复图片底部空白 */
.morebox image {
    display: block;
}

.boxmask1{
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: rgba(0, 0, 0, .8);
    z-index: 990;
    height: 100%;
    overflow-y: scroll;
    overflow-x: hidden;
}

.boxmask-content1 {
    position: fixed;
    top: 50%;
    left: 50%;
    -webkit-transform: translate3d(-50%, -50%, 0);
    transform: translate3d(-50%, -50%, 0);
    width: 100%;
    z-index: 12;
}
.boxmask-content2{
    position: absolute;
    left:50%;
    top:50%;
    transform: translate(-50%,-50%);
}
.boxmask-content1 .sharetitle {
    width: 300rpx;
    height: 60rpx;
    margin: 30rpx auto;
    background: var(--theme-color, #185bdd);
    text-align: center;
    font-size: 28rpx;
    line-height: 60rpx;
    border-radius: 30rpx;
}

.boxmask-content1 .close {
    width: 64rpx;
    height: 64rpx;
    border-radius: 50%;
    margin: 0 auto;
    line-height: 80rpx;
    font-size: 36rpx;
    background-image: url("https://guapikeji-1308841152.cos.ap-shanghai.myqcloud.com/assets/img/icon/common/closedx.png");
    background-size: 64rpx 64rpx;
    margin-top: 20rpx;
}


.share::after {
    border: none !important;
}


/* 弹框 */
.applypupop_mask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, .8);
  z-index: 1000;
}
.applypupop_mask .negativefactormask-content {
  position: fixed;
  top: 50%;
  left: 50%;
  -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
  width: 690rpx;
  /* height: 659rpx; */
  border-radius: 8rpx;
  overflow: hidden;
  z-index: 101;
  text-align: center;
}
.applypupop_mask .negativefactor-closed {
  width: 60rpx;
  height: 60rpx;

}
.applypupop_mask .negativefactorcontent {
  display: block;
  width: 690rpx;
  margin: 0rpx 0rpx 30rpx 0rpx;
  box-sizing: border-box;
  text-align: center;
}
.applypupop_mask .resultsuccess {
  display: inline-block;
  /* width: 235rpx; */
  /* height: 266rpx; */
  margin-top: 8rpx;
}
.applypupop_mask .negativefactorcontent .tit {
  color: #808080;
  line-height: 40rpx;
  font-size: 28rpx;
  letter-spacing: 4rpx;
}
.applypupop_mask .negativefactorcontent .button {
  border: none;
  outline: none;
  background: var(--theme-color, #185bdd);
  color: #fff;
  width: 240rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  text-align: center;
  margin-top: 50rpx;
}

