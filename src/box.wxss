/* 问答 */
.boxquestionmask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 1001;
}

.boxquestionmask .applymask-content {
  position: fixed;
  top: 50%;
  left: 50%;
  -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
  width: 640rpx;
  /* height: 890rpx; */
  border-radius: 8rpx;
  overflow: hidden;
  z-index: 100;
  background: #fff;
}

.boxquestionmask .apply-closed {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  width: 32rpx;
  height: 31rpx;
}
.boxquestionmask .applycontent {
  display: block;
  width: 580rpx;
  /* height: 800rpx; */
  margin: 60rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box;
  overflow: scroll;
}
.boxquestionmask .applycontent .applyinput {
  width: 100%;
  height: 180rpx;
  background: rgba(244, 244, 244, 1);
  border-radius: 4px;
  margin: 20rpx 0rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
}

.boxquestionmask .applycontent .button {
  border: none;
  outline: none;
  background: var(--theme-color, #185bdd);
  color: #fff;
  width: 230rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  text-align: center;
  margin-top: 30rpx;
}


.boxquestionmask .qa-tit{
  font-size: 28rpx;
  font-weight: bold;
  color: #808080;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.boxquestionmask .qatag-content{
  display: flex;
  align-items: flex-start;
  margin-bottom: 20rpx;
}
.boxquestionmask .qa-title {
  font-size: 26rpx;
  color: #808080;
  line-height: 42rpx;
  box-sizing: border-box;
}

.boxquestionmask .qa_columlist {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
 
  padding-left: 30rpx;
  box-sizing: border-box;
}
.boxquestionmask .qa_colum_view {
  display: block;
  background: #F0F0F0;
  border-radius: 8rpx;
  font-size: 24rpx;
  color: #808080;
  line-height: 32rpx;
  text-align: center;
  box-sizing: border-box;
  margin-right: 20rpx;
  padding:6rpx 10rpx;
  border: 1rpx solid #F0F0F0;
  margin-bottom: 10rpx;
}
.boxquestionmask .qa_colum_view.active {
  font-size: 24rpx;
  font-weight: bold;
  color: #808080;
  line-height: 32rpx;
  background: #fff;
  border-radius: 8rpx;
  border: 1rpx solid var(--theme-color, #185bdd);
  box-sizing: border-box;
  text-align: center;
}


.pay-topic-bg {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  z-index: 9999999
}
.pay-topic-bg .location-inner {
  background: #fff;
  position: fixed;
  bottom: 0;
  width: 100%;
  border-radius: 40rpx 40rpx 0rpx 0rpx;
  padding-bottom:40rpx;
}


.pay-topic-bg .new-location-label {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(92, 92, 92, 1);
}
.pay-topic-bg .close {
  width: 690rpx;
  height: 31rpx;
  padding: 30rpx 30rpx 0rpx 30rpx;
  text-align: right;
}
.pay-topic-bg .arrow {
  width: 32rpx;
  height: 31rpx;
}
.pay-topic-bg .location-img {
  width: 120rpx;
  height: 120rpx;
}

.pay-topic-bg .another {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  max-height: 800rpx;
  overflow-y: scroll;
}
.pay-topic-bg .another-item {
  /* width:217rpx; */
  padding:0 10rpx;
  height: 62rpx;
  background: #F0F0F0;
  border-radius: 8rpx;
  margin-right:20rpx;
  font-size: 22rpx;
  color: #5A5A5A;
  line-height: 62rpx;
  text-align: center;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}
.pay-topic-bg .another-item:nth-child(3n) {
  margin-right: 0rpx;
}
.pay-topic-bg .another-item.active{
  background: #fff;
  border-radius: 8rpx;
  border: 1rpx solid var(--theme-color, #185bdd);
  box-sizing: border-box;
  font-weight: bold;
  color: var(--theme-color, #185bdd);
}
.pay-topic-bg .title {
  font-size: 32rpx;
  font-weight: 400;
  color: #3D3D3D;
  line-height: 40rpx;
}
.pay-topic-bg .orderconfirm{
  width: 690rpx;
  margin:0 auto;
  background: var(--theme-color, #185bdd);
  border-radius: 35rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
  padding: 20rpx 0rpx;
}
